set SERVER_NAME=Server1

%windir%\system32\inetsrv\appcmd.exe list apppool /state:Started | %windir%\system32\inetsrv\appcmd.exe recycle apppool /in /t "%SERVER_NAME%"

set SERVER_NAME=Server1

%windir%\system32\inetsrv\appcmd.exe list apppool /state:Started | findstr /I "appPool.name" | %windir%\system32\inetsrv\appcmd.exe recycle apppool /t "%SERVER_NAME%"
